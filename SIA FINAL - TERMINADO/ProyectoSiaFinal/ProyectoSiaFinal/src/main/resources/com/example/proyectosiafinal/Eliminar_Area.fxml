<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="292.0" prefWidth="360.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectosiafinal.EliminarAreaController">
    <children>
        <Label layoutX="130.0" layoutY="14.0" text="Eliminar Área" />
        <Label layoutX="18.0" layoutY="125.0" text="Ingrese Nombre de Área:" />

        <TextField fx:id="tfArea" layoutX="163.0" layoutY="121.0" prefHeight="25.0" prefWidth="149.0" />

        <Button fx:id="btnEliminar" layoutX="18.0" layoutY="240.0" prefHeight="25.0" prefWidth="100.0"
                text="Eliminar" onAction="#onEliminar"/>
        <Button fx:id="btnSalir" layoutX="294.0" layoutY="240.0" text="Salir" onAction="#onSalir"/>
    </children>
</Pane>
